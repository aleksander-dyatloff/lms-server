{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
      "import/resolver": {
        "typescript": {
          "extensions": [".js", ".ts"]
        }
      }
    },
    "rules": {
      "import/extensions": "off",
      "no-console": "off",
      "consistent-return": "off",
      "import/prefer-default-export": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-restricted-syntax": "off"
    }
}
